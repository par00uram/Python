list=[76,54,98,34,55,32]
list1=[11,22,33,44,55]
print(list)
print(list[2])
print(list[2:5])
print(list[::-1])
list.append(100)
print(list)
list.extend(list1)
print(list)
list.insert(2,200)
print(list)
list.pop(2)
print(list)
list.remove(100)
print(list)




aLsit = [100, 200, 300, 400, 500]
aLsit.reverse()
print(aLsit)





lst=[1,2,3,4,5,6]
lst[0],lst[5]=lst[5],lst[0]
print(lst)




lst=[1,2,3,4,5,6,-4]
print(min(lst))
